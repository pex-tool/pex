#!python
# -*- coding: utf-8 -*-
import importlib
import sys

entry_point = importlib.import_module('setuptools.command.easy_install')
for attr in ('main',):
    entry_point = getattr(entry_point, attr)

if __name__ == "__main__":
    import os
    pex_root_fallback = os.environ.get("_PEX_ROOT_FALLBACK")
    if pex_root_fallback:
        import atexit
        import shutil

        atexit.register(shutil.rmtree, pex_root_fallback, True)

    sys.exit(entry_point())
